{
    "name": "NicGAG WEB",
    "developer": {
        "name": "kapodamy",
        "url": "taringa.net/lossles_kapodamy"
    },
    "default_locale": "en",
    "description": "__MSG_description__",
    "manifest_version": 2,
    "version": "0.0.20",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "downloads",
        "*://9gag.com/*"
    ],
    "background": {
        "scripts": ["nicgag_loader.js", "service.js"]
    },
    "icons": {
        "32":  "/icons/icon-32.png",
        "64": "/icons/icon-64.png",
        "128": "/icons/icon-128.png"
    },
    "content_scripts": [{
        "matches": ["*://9gag.com/*"],
        "js": ["downloads.js"],
        "run_at": "document_end"
    }],
    "web_accessible_resources": ["nicgag.html", "nicgag.webmanifest", "*.js", "*.css", "templates/*.png", "splash_screen.svg", "templates/*.webm"],
    "applications": {
        "gecko": {
            "id": "NicGAGWEB@kapodamy",
            "strict_min_version": "68.0"
        }
    }
}